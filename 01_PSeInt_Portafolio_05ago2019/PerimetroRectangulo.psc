Algoritmo PerimetroRectangulo
	Leer largo // variable lado largo
	Leer ancho // variable largo ancho
	
	perimetro <- (largo * 2) + (ancho * 2) // proceso calcular perímetro
	Escribir perimetro " cm de perimetro" // imprimir
FinAlgoritmo
